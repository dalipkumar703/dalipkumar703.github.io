let arr = [1, 2, 3, 7, 5];
//sliding window approach
function calculateSum(arr, num){
let left = 0;
let right = 0;
let sum = 0;
while (right < arr.length){
sum += arr[right];
while (sum > num){
sum -= arr[left];
left++;
//remove starting elmenet and increase left position
}
if(sum === num){
return [left, right];
}
right++;
//increase right position
}
return [-1];

}
console.log(calculateSum(arr,12));
